    <name short="Gemairo">Gemairo - Stats voor Magister</name>
    <description>
        Shi daggoetje geswiped
    </description>
    <author email="sjoerdabolten@gmail.com" href="https://sjoerd.dev">
        Sjoerd Bolten
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="mailto:*" />
    <config-file parent="UIUserInterfaceStyle" platform="ios" target="*-Info.plist">
        <string>Light</string>
    </config-file>
    <allow-intent href="m6loapp://oauth2redirect/*" />
    <edit-config file="*-Info.plist" mode="merge" target="UIUserInterfaceStyle">
        <string>Light</string>
    </edit-config>
    <edit-config file="*-Info.plist" mode="merge" target="NSUserTrackingUsageDescription">
        <string>Dit gebruikt Gemairo om jouw app ervaring te verbeteren en relevante
                advertenties te vinden.</string>
    </edit-config>
